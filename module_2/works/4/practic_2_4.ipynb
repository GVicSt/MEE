    "def limit(x,formule):\n",
    "    p = formule.split('/')\n",
    "    for i in range(len(p)):\n",
    "        if(eval(p[-1-i]) == 0):\n",
    "            return '0/0'\n",
    "    else:\n",
    "        return eval(formule)\n",
    "\n",
    "print(limit(int(input()),'((x*x*x)+5*(x*x)+(3*x))/(3*x)'))"
   ]
  }
